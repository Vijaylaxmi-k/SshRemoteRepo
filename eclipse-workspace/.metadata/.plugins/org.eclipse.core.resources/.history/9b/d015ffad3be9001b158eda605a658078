package Pakage4;

public class Totalmaths 
{

	public int add(int a, int b)
	{
	return a+b;
	}

	public int factorial(int n)
	{
		int i=1, fact=1; 
		for(i=1;i<=1;i++)
			fact=fact*i;
		return fact;		
	}
	
	public int mul(int a, int b)
	{
		return a*b;
	}
	
	public int power(int a, int b)
	{
		int i, result=1;
		for(i=1; i<=b; i++)
			result=result*a;
     		return result;
	}
	
	public int subtraction(int a, int b)
	{
		return a-b;
		
	}

}
